FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
simulationType  RAS;//laminar;//

RAS
{
    RASModel      kOmegaSST;// SpalartAllmaras;//kOmegaSST;//
    turbulence      on;
    betannmodel 0;
    Main         9;
    betascalar   1;
    begin 0.1;
    printCoeffs     on;
}



FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    intPatch
    {
        type            zeroGradient;
    }
    wing
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    symm
    {
        type            symmetry;
    }
    farfield
    {
        type            calculated;
        value           uniform 0;
    }
}



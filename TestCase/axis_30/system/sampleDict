FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
type sets;
libs ("libsampling.so");

setFormat raw;
surfaceFormat vtk;
interpolationScheme cellPoint;

fields
(
    U
);

sets
(
    line_x0
    {
        type        midPoint;
        axis        y;
        start       (-0.19 0 0);
        end         (-0.19 3.1 0);
    }
);


